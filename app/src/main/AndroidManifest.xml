<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.jrn.vocabloom">
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@drawable/vocabloom" android:theme="@style/AppTheme"
        android:debuggable="true" android:exported="true">
        <activity android:name="com.jrn.vocabloom.main_menu" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jrn.vocabloom.scan" android:exported="true"/>
        <activity android:name="com.jrn.vocabloom.results" android:exported="true"/>
        <activity android:name="com.jrn.vocabloom.scan_successful" android:exported="true"/>
        <activity android:name="com.jrn.vocabloom.scan_history" android:exported="true"/>
        <activity android:name="com.jrn.vocabloom.synonyms" android:exported="true"/>
        <activity android:name="com.jrn.vocabloom.clarification" android:exported="true"/>
    </application>

</manifest>
